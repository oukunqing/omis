<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!-- 日志文件大小，单位：M -->
    <add key="LogFileSize" value="10"/>
    <!--日志配置开始-->
    <add key="ErrorLogDir" value="log/error/"/>
    <add key="EventLogDir" value="log/event/"/>
    <add key="DebugLogDir" value="log/debug/"/>
    <add key="SaveErrorLog" value="1"/>
    <add key="SaveEventLog" value="1"/>
    <add key="SaveDebugLog" value="1"/>
    <add key="LogKeepDays" value="3"/>
  </appSettings>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
