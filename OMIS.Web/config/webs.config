  <add key="SaveUserName" value="1" />
  <!--是否保存用户密码，0-不保存，1-保存-->
  <add key="SaveUserPassword" value="0" />
  <!--是否启用后台自动登录，0-不启用，1-启用-->
  <add key="UserAutoLogin" value="0" />
  <!--登录失败多少次启用验证码，若设置为-1表示不启用验证码-->
  <add key="LoginFailedTimes" value="3" />
  <!--登录失败多少次后锁定帐户，若设置为-1表示不锁定-->
  <add key="LoginFailedLocked" value="10" />
  <!--登录失败后锁定帐户多少时间，单位：秒，默认为15分钟-->
  <add key="LoginFailedLockedTime" value="900" />
  <!--用户登录COOKIE有效时间，单位：分钟，默认为1440分钟（1天）-->
  <add key="UserLoginCookieExpireTime" value="1440" />
  <!--检测用户登录状态最大有效期时间（超出时间将强制登出），单位：分钟，-1表示不检测有效期-->
  <add key="UserLoginStatusMaxExpireTime" value="-1" />
