  <add key="CheckUserExpireTime" value="1" />
  <!--是否启用用户自定义首页，0-不启用，1-启用-->
  <add key="EnabledUserCustomHome" value="0" />
  <!--是否启用密码闪现效果（仿手机密码输入效果），1-启用，0-不启用-->
  <add key="EnabledPasswordFlash" value="0" />
  <!--系统管理员（非超级管理员）是否可以控制“系统设置”模块中的设置功能-->
  <add key="AdminEnabledControlSystemConfig" value="0" />
  <!--是否验证数据来源-->
  <add key="ValidateUrlReferrer" value="0" />
  <!--是否验证表单提交方式-->
  <add key="ValidateRequestType" value="1" />
  <!--是否验证表单请求令牌-->
  <add key="ValidateRequestToken" value="0" />
  
