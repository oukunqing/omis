  <add key="ConfigFileDir" value="/config" />
  <add key="ConfigFileName" value="webs.config" />
  <add key="NetFrameVersion" value="4.0" />
  <!--程序版本-->
  <add key="Version" value="2015.07.27" />
  <!--程序是否正在升级-->
  <add key="Upgrade" value="0" />
