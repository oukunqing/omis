  <!--系统配置-->
  <add key="WebName" value="在线信息管理系统" />
  <add key="LogoImage" value="logo.png?150401113000" />
  <add key="LogoName" value="" />
  <add key="Copyright" value="版权所有 &amp;#169; 2015-{0} 在线信息管理系统" />
  <!--系统图片配置-->
  <add key="LoginBg" value="login-bg.jpg?150401112536" />
  <add key="WelcomeBg" value="welcome.jpg" />
  <add key="TopBanner" value="banner.png" />
  <add key="IndexTopBanner" value="banner-index.jpg?140816172542" />
  <!--模块是否启用选项卡模式-->
  <add key="ModuleMultiPage" value="0" />
  <!--是否允许关闭第一个加载的模块选项卡-->
  <add key="AllowFirstClose" value="0" />
  <!--选项卡页面最小数量-->
  <add key="ModuleTabMinCount" value="1" />
  <!--选项卡页面最大数量-->
  <add key="ModuleTabMaxCount" value="10" />
  <!--是否启用选项卡模式-->
  <add key="MultiPage" value="1" />
  <!--选项卡页面最小数量-->
  <add key="TabMinCount" value="0" />
  <!--选项卡页面最大数量-->
  <add key="TabMaxCount" value="20" />
  <!--选项卡标签文字显示字数（字节）-->
  <add key="TabWordMaxLength" value="20" />
  <!--是否禁用右键菜单-->
  <add key="DisabledContextMenu" value="0" />
  <!--设置表格最小宽度（为了表格不变形）-->
  <add key="SetTableMinWidth" value="1" />
  <!--数据列表默认显示行数-->
  <add key="ListPageSize" value="20" />
  <!--列表分页设置-->
  <add key="PageMarkType" value="Symbol" />
  <add key="PageMarkCount" value="10" />
  <add key="HidePageUrl" value="1" />
  <add key="ShowPageCount" value="1" />
  <add key="ShowDataCount" value="1" />
  <add key="ShowPageJump" value="1" />
